<%- include ./partials/header %>

<div class='d-flex justify-content-center'>
  <form action='/login' method='post' class="p-2 border">
    <div class="form-group">
      <label for="username">Username</label>
      <input type="text" class="form-control" id="username" name="username" placeholder="Test User Name: 1">
    </div>
    <div class="form-group">
      <label for="password">Password</label>
      <input type="password" class="form-control" id="password" name='password' placeholder="Test Password: 1">
    </div>
    <div class='text-center'>
      <button type="submit" class="btn btn-primary">Submit</button>
    </div>
  </form>
</div>

<%- include ./partials/footer %>
